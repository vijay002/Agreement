<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/jqueryui/themes/ui-lightness/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/lib/jqueryui/themes/ui-lightness/theme.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />

</head>
<body>
    <div>
        @RenderBody()
    </div>
    @RenderSection("Scripts", false)

</body>

<environment exclude="Development">

</environment>
<environment include="Development">
    
    <script src="~/lib/jquery/dist/jquery.min.js" type="text/javascript" asp-append-version="false"></script>
    <script src="~/lib/jqueryui/jquery-ui.min.js" type="text/javascript" asp-append-version="false"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" asp-append-version="false"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js" asp-append-version="true"></script>

</environment>
@RenderSection("Scripts", false);

</html>
